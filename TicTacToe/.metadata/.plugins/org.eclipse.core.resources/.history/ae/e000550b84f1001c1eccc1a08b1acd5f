package com.springboot.app.board.utils;

import com.springboot.app.board.dto.BoardDto;
import com.springboot.app.board.entities.BoardEntity;
import com.springboot.app.player.dtos.PlayerDto;
import com.springboot.app.player.entities.PlayerEntity;

public class MapperBoard {
	
	public BoardDto toDto(BoardEntity boardEntity) {
		BoardDto boardDto = new BoardDto();
		boardDto.setDimension(boardEntity.getDimension());
		return boardDto;
	}
	
